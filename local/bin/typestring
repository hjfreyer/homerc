#!/bin/sh

K1=$'\300'
K2=$'\301'
K3=$'\302'
K4=$'\303'
K5=$'\304'

(echo "KeyCodeRelease $K1" && 
    echo "KeyCodeRelease $K2" && 
    echo "KeyCodeRelease $K3" && 
    echo "KeyCodeRelease $K4" && 
    echo "KeyCodeRelease $K5" && 
    echo "string $1") | xmacroplay -d 0 ":0.0" 2> /dev/null > /dev/null


